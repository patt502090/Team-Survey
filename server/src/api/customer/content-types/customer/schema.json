{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "NameTitle": {
      "type": "enumeration",
      "enum": [
        "Mr.",
        "Mrs.",
        "Master",
        "Miss"
      ],
      "required": true
    },
    "Name_Th": {
      "type": "string",
      "required": true
    },
    "Name_Eng": {
      "type": "string",
      "required": true
    },
    "Address": {
      "type": "string",
      "required": true
    },
    "Id_Number": {
      "type": "string",
      "required": true,
      "maxLength": 13,
      "minLength": 13
    },
    "birthdate": {
      "type": "date",
      "required": false
    },
    "business_with": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "customers"
    },
    "estimate": {
      "type": "enumeration",
      "enum": [
        "green",
        "yellow",
        "red"
      ]
    },
    "Pic": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Assessor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "my_customers"
    },
    "sub_district": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-district.sub-district",
      "inversedBy": "customers"
    }
  }
}
